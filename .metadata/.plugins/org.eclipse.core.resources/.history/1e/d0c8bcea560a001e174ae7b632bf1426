package application;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		list.add("Marco");
		list.add("Anna");
		list.add("Bob");
		list.add("Alex");
		
		list.removeIf(person -> person.charAt(0) == 'B');

	}

}
