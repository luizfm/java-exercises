package entities;

public class Room {
	private int roomNumber;
	private boolean isBusy;
	
	public Room(int roomNumber) {
		this.roomNumber = roomNumber;
		setFree();
	}
	
	public int getRoomNumber() {
		return this.roomNumber;
	}
	
	public boolean getIsBusy() {
		return this.isBusy;
	}
	
	public void setBusy() {
		this.isBusy = true;
	}
	
	public void setFree() {
		this.isBusy = false;
	}
}
